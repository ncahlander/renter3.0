<% @users = User.all %>
<% @users.each do |user| %>
  <%= link_to "view profile", user.profile%>
  <%= user.firstName %><br/>
  <%= user.lastName %><br/>
  <%= user.email %><br/>
  <br>
<% end %>

<!-- If search for users comes up blank -->
<% if @users.blank? %>
  <h4>There are no accounts containing the email, username, first, or last name: <%= params[:search] %>.</h4>
<% end %>
